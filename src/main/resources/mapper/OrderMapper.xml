<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.palettone.shoppingwebsite.mapper.OrderMapper">
    <select id="selectOrderByUserId" resultType="xyz.palettone.shoppingwebsite.entity.Order">
        SELECT *
        FROM  tb_order
        WHERE user_id = #{userId}
    </select>


</mapper>